,"master_URLs =  [\n","    \"https://www.espncricinfo.com/series/icc-cricket-world-cup-2023-24-1367856/match-schedule-fixtures-and-results\",\n","\n","     ]\n","\n","def url_thread(url1):\n","    #print(url1)\n","    page_text = getURLText([url1])\n","    while True:\n","      url, n = getURL(page_text)\n","      page_text = page_text[n:]\n","      if url:\n","        if not url.startswith(\"https://\"):\n","          url = \"https://www.espncricinfo.com\" + url\n","          if \"icc-cricket-world-cup\" in url and demo_filter_text in url:\n","              urls_extracted.append(url)\n","              #print(url)\n","      else:\n","          break\n","\n","\n","def url_extractor(URLsToIterate, depth):\n","  demo_filter_text = \"vs\"\n","\n","  for i in range(depth):\n","\n","\n","    threads = list()\n","    for index in range(len(URLsToIterate)):\n","        #print(index)\n","        #logging.info(\"Main    : create and start thread %d.\", index)\n","        x = threading.Thread(target=url_thread, args=(URLsToIterate[index],))\n","        threads.append(x)\n","        x.start()\n","\n","    for index, thread in enumerate(threads):\n","        #logging.info(\"Main    : before joining thread %d.\", index)\n","        thread.join()\n","        #logging.info(\"Main    : thread %d done\", index)\n","\n","\n","    #urls_extracted = list(dict.fromkeys(urls_extracted))\n","    #for url in urls_extracted:\n","    #  print(url)\n","    urls_extracted2 = list(dict.fromkeys(urls_extracted))\n","    urls.extend(urls_extracted2)\n","    URLsToIterate = urls_extracted2\n","    print (\"now urls to iterate are \", len(URLsToIterate))\n","\n","\n","url_extractor(master_URLs, 2)\n","print(len(urls))\n","urls.extend(master_URLs)\n","print(len(urls))\n","urls = list(dict.fromkeys(urls))\n","print(len(urls))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"vNEzN_o3Ns1J","executionInfo":{"status":"ok","timestamp":1699887313533,"user_tz":-540,"elapsed":35474,"user":